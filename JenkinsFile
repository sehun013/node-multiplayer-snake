node {
                    stage('SCM') {
                        checkout scm 
                    }
                    stage('Start') {
                        slackSend (channel: '#jenkins', color: '#FFFF00', message: "STARTED: Job '${env.JOB_NAME} [${env.BUILD_NUMBER}]' (${env.BUILD_URL})")
                    }
                    stage('Scanner') {
                        def scannerHome = tool 'Scanner';
                        withSonarQubeEnv('SonarServer') { 
                            sh "${scannerHome}/bin/sonar-scanner"
                        }
                    }
                }